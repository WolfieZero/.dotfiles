[user]
	name = Neil Sweeney
	email = neil@wolfiezero.com
	signingKey = ""
[core]
	excludesfile = /Users/neil/.gitignore
[github]
	user = WolfieZero
[init]
	defaultBranch = main
[includeIf "gitdir:~/Refiine/"]
	path = ~/Refiine/.gitconfig
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[gpg]
	program = gpg
[commit]
	gpgSign = false
[tag]
	forceSignAnnotated = false
